package org.jnit.jdbc;

import java.util.*;

public class Doctor 
{
	private int doc_id;
	private String doc_name;
	private String category;
	private List<TreatmentInfo> tList=new ArrayList<>();
	private List<Hospital> hospitals = new ArrayList<>();
	
	public List<Hospital> getHospitals() {
		return hospitals;
	}
	public void setHospitals(List<Hospital> hospitals) {
		this.hospitals = hospitals;
	}
	public int getDoc_id() {
		return doc_id;
	}
	public void setDoc_id(int doc_id) {
		this.doc_id = doc_id;
	}
	public String getDoc_name() {
		return doc_name;
	}
	public void setDoc_name(String doc_name) {
		this.doc_name = doc_name;
	}
	public String getCategory() {
		return category;
	}
	public void setCategory(String category) {
		this.category = category;
	}
	public List<TreatmentInfo> gettList() {
		return tList;
	}
	public void settList(List<TreatmentInfo> tList) {
		this.tList = tList;
	}
	@Override
	public String toString() {
		return "Doctor [doc_id=" + doc_id + ", doc_name=" + doc_name + ", category=" + category + ", tList=" + tList
				+ ", hospitals=" + hospitals + "]";
	}
		
	
	

}
